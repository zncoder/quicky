{
  "name": "Single Key Browsing",
  "version": "1.0.1",
  "manifest_version": 2,
  "description": "A few single key shortcuts for common browsing actions",
  "icons": {
    "48": "icons/48.png"
  },
  "background": {
    "scripts": [ "background.js" ]
  },
  "content_scripts": [
    {
      "js": [ "page.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
    }
  ]
}
